{
  "name": "hackur/jeremysarda-sage-theme",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "JeremySarda.com Wordpress Blog Theme",
  "homepage": "https://github.com/hackur/jeremysarda-sage-theme",
  "authors": [
    {
      "name": "Jeremy Sarda",
      "email": "jcsarda@gmail.com",
      "homepage": "https://jeremysarda.com"
    }
  ],
  "keywords": [
    "wordpress sage roots bootstrap bs4"
  ],
  "support": {
    "issues": "https://github.com/hackur/jeremysarda-sage-theme/issues"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Sage\\": "src/lib/Sage/"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "phpmd/phpmd": "^2.4.2"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs",
      "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming,unusedcode"
    ]
  }
}
